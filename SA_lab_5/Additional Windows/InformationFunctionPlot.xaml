<Window x:Class="SA_lab_5.InformationFunctionPlot"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:oxy="http://oxyplot.org/wpf"
        xmlns:local="clr-namespace:SA_lab_5"
        mc:Ignorable="d"
        Title="(None)" Height="300" Width="900">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
        </Grid.ColumnDefinitions>
        <oxy:PlotView x:Name="fullnessPlot" Grid.Column="0"/>
        <oxy:PlotView x:Name="reliabilityPlot" Grid.Column="1"/>
        <oxy:PlotView x:Name="timelinessPlot" Grid.Column="2"/>
    </Grid>
</Window>
